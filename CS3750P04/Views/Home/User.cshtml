
@model IEnumerable<CS3750P04.Models.User>
@*
This specifies the model type that this view can take.
*@

@{
    ViewData["Title"] = "User";
}

<h2>User</h2>

<table>
        <tr>
            <th>Screen Name</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.ScreenName</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
        </tr>
    }
</table>